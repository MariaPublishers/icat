<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context=".splash_screens.JesusIntro">


    <VideoView
        android:id="@+id/jesus_video"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true"
        android:layout_above="@id/enter_button"/>


    <Button
        android:id="@+id/enter_button"
        style="@style/EnterButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="@dimen/ten"
        android:background="@drawable/curve_button"
        android:onClick="enterClicked"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/twenty_five"
        android:layout_marginEnd="@dimen/twenty_five"
        tools:ignore="MissingConstraints" />


</RelativeLayout>